{
  "expo": {
    "name": "MOS Controller",
    "runtimeVersion": "1.0.0",
    "slug": "mos-controller",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "light"
    },
    "android": {
      "userInterfaceStyle": "light",
      "package": "com.unsignedinteger.moscontroller",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": ["./plugins/withDisableForcedDarkModeAndroid.js"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2b52880a-b247-416e-ae53-049a0a9bdfda"
      }
    },
    "owner": "unsignedinteger"
  }
}
